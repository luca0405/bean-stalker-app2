# Production Environment Variables for Bean Stalker
# Copy these to your production deployment environment

# Core App Configuration
NODE_ENV=production
VITE_APP_TITLE="Bean Stalker"
VITE_APP_VERSION="1.0.0"

# RevenueCat Configuration (Sandbox for Testing)
VITE_REVENUECAT_API_KEY=appl_owLmakOcTeYJOJoxJgScSQZtUQA

# Production Logging Controls
ENABLE_IAP_LOGGING=false
VITE_ENABLE_IAP_LOGS=false
ENABLE_DEBUG_MODE=false

# Square Production Credentials (Already configured in secrets)
# SQUARE_ACCESS_TOKEN=<production_token>
# SQUARE_APPLICATION_ID=<production_app_id>
# SQUARE_LOCATION_ID=<production_location_id>
# SQUARE_WEBHOOK_SIGNATURE_KEY=<production_webhook_key>

# Feature Flags for Production
ENABLE_SQUARE_INTEGRATION=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_IAP_DIAGNOSTICS=true

# Database Configuration (Already configured)
# DATABASE_URL=<postgresql_connection_string>

# Email Configuration (Already configured)
# GMAIL_USER=<gmail_address>
# GMAIL_PASS=<gmail_app_password>

# Push Notification Keys (Already configured)
# VAPID_PUBLIC_KEY=<vapid_public_key>
# VAPID_PRIVATE_KEY=<vapid_private_key>